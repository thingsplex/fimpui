      "val_t": "string",
      "ui": {
        "type": "input_string"
      },
      "val": {
        "default": ""
      },
      "is_required": false,
      "hidden":false,
      "config_point": "any"
    },{
      "id": "password",
      "label": {"en": "password"},
      "val_t": "string",
      "ui": {
        "type": "input_string"
      },
      "val": {
        "default": ""
      },
      "is_required": false,
      "hidden":false,
      "config_point": "any"
    }
